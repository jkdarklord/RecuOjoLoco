package test;

import java.util.Vector;

public class PostingsList
{
	public class PostingsListElement
	{
		public int docID;
		public int termID;
		public int df;
	}
	
	public Vector<PostingsListElement> postings;
	
	public PostingsList()
	{
		postings = new Vector<PostingsListElement>();
	}
	
	public PostingsListElement append(PostingsListElement p)
	{
		PostingsListElement p = new ();
		p.docID = docID;
		p.termID = termID;
		p.df = df;
		postings.add(p);
		return p;
	}
}
